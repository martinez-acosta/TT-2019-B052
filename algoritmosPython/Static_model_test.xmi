<?xml version='1.0' encoding='UTF-8'?>
<gdmLang:Model xmlns:xmi="http://www.omg.org/XMI" xmlns:gdmLang="http://www.unican.es/istr/mortadelo/gdm/lang/GdmLang" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmi:version="2.0">
  <queries xsi:type="gdmLang:Query" name="Q1_artifactsByVenue">
    <inclusions xsi:type="gdmLang:Inclusion" refAlias="//@queries.0/@from_/@alias" refs="//@entities.1/@features.6">
      <alias xsi:type="gdmLang:Alias" name="artifacts"/>
    </inclusions>
    <from_ xsi:type="gdmLang:From" entity="//@entities.1">
      <alias xsi:type="gdmLang:Alias" name="venue"/>
    </from_>
    <condition xsi:type="gdmLang:AndConjunction">
      <right xsi:type="gdmLang:MoreThan" value="?">
        <selection xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@from_/@alias" attribute="//@entities.1/@features.2"/>
      </right>
      <left xsi:type="gdmLang:Equality" value="?">
        <selection xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@from_/@alias" attribute="//@entities.1/@features.1"/>
      </left>
    </condition>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@from_/@alias" attribute="//@entities.1/@features.1"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@from_/@alias" attribute="//@entities.1/@features.2"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@inclusions.0/@alias" attribute="//@entities.2/@features.0"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@inclusions.0/@alias" attribute="//@entities.2/@features.1"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@inclusions.0/@alias" attribute="//@entities.2/@features.6"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@inclusions.0/@alias" attribute="//@entities.2/@features.2"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.0/@inclusions.0/@alias" attribute="//@entities.2/@features.3"/>
  </queries>
  <queries xsi:type="gdmLang:Query" name="Q3_usersByLikedArtifact">
    <inclusions xsi:type="gdmLang:Inclusion" refAlias="//@queries.1/@from_/@alias" refs="//@entities.0/@features.5">
      <alias xsi:type="gdmLang:Alias" name="artifacts"/>
    </inclusions>
    <from_ xsi:type="gdmLang:From" entity="//@entities.0">
      <alias xsi:type="gdmLang:Alias" name="user"/>
    </from_>
    <condition xsi:type="gdmLang:AndConjunction">
      <left xsi:type="gdmLang:Equality" value="?">
        <selection xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.1/@inclusions.0/@alias" attribute="//@entities.2/@features.0"/>
      </left>
    </condition>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.1/@from_/@alias" attribute="//@entities.0/@features.0"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.1/@from_/@alias" attribute="//@entities.0/@features.1"/>
    <projections xsi:type="gdmLang:AttributeSelection" refAlias="//@queries.1/@from_/@alias" attribute="//@entities.0/@features.2"/>
  </queries>
  <queries xsi:type="gdmLang:Query" name="Q2_artifactsByAuthor">
    <inclusions xsi:type="gdmLang:Inclusion" refAlias="//@queries.2/@from_/@alias">
      <alias xsi:type="gdmLang:Alias" name="artifacts"/>
    </inclusions>
    <from_ xsi:type="gdmLang:From" entity="//@entities.2">
      <alias xsi:type="gdmLang:Alias" name="artifact"/>
    </from_>
  </queries>
  <queries xsi:type="gdmLang:Query" name="Q4_expertsByLikedArtifact"/>
  <queries xsi:type="gdmLang:Query" name="Q5_ratingByArtifact"/>
  <queries xsi:type="gdmLang:Query" name="Q6_venuesLikedByUser"/>
  <queries xsi:type="gdmLang:Query" name="Q7_artifactsLikedByUser"/>
  <queries xsi:type="gdmLang:Query" name="Q8_reviewsByUser"/>
  <queries xsi:type="gdmLang:Query" name="Q9_artifacts"/>
  <entities xsi:type="gdmLang:Entity" name="User">
    <features xsi:type="gdmLang:Attribute" unique="true" name="userId" type="ID"/>
    <features xsi:type="gdmLang:Attribute" name="userName" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="userEmail" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="areasOfExpertise" type="TEXT" cardinality="*"/>
    <features xsi:type="gdmLang:Reference" name="reviews" entity="//@entities.3" cardinality="*"/>
    <features xsi:type="gdmLang:Reference" name="likesArtifacts" entity="//@entities.2" cardinality="*"/>
    <features xsi:type="gdmLang:Reference" name="likesVenue" entity="//@entities.1" cardinality="*"/>
  </entities>
  <entities xsi:type="gdmLang:Entity" name="Venue">
    <features xsi:type="gdmLang:Attribute" unique="true" name="venueId" type="ID"/>
    <features xsi:type="gdmLang:Attribute" name="venueName" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="year" type="NUMBER"/>
    <features xsi:type="gdmLang:Attribute" name="country" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="homepage" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="topics" type="TEXT" cardinality="*"/>
    <features xsi:type="gdmLang:Reference" name="artifacts" entity="//@entities.2" cardinality="*"/>
  </entities>
  <entities xsi:type="gdmLang:Entity" name="Artifact">
    <features xsi:type="gdmLang:Attribute" unique="true" name="artifactId" type="ID"/>
    <features xsi:type="gdmLang:Attribute" name="artifactTitle" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="authors" type="TEXT" cardinality="*"/>
    <features xsi:type="gdmLang:Attribute" name="keywords" type="TEXT" cardinality="*"/>
    <features xsi:type="gdmLang:Attribute" name="numRatings" type="NUMBER"/>
    <features xsi:type="gdmLang:Attribute" name="sumRatings" type="NUMBER"/>
    <features xsi:type="gdmLang:Attribute" name="avgRating" type="NUMBER"/>
    <features xsi:type="gdmLang:Reference" name="venue" entity="//@entities.1" cardinality="1"/>
  </entities>
  <entities xsi:type="gdmLang:Entity" name="Review">
    <features xsi:type="gdmLang:Attribute" unique="true" name="reviewId" type="ID"/>
    <features xsi:type="gdmLang:Attribute" name="reviewTitle" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="body" type="TEXT"/>
    <features xsi:type="gdmLang:Attribute" name="rating" type="NUMBER"/>
    <features xsi:type="gdmLang:Reference" name="artifact" entity="//@entities.2" cardinality="1"/>
  </entities>
</gdmLang:Model>
